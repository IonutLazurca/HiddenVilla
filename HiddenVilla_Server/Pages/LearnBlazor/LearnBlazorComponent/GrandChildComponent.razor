<div class="border mt-2">
    <div class="row">
        <h4 class="text-primary pl-4 pt-2 col-12">Grand Child Component</h4>   
    </div>
    <div class="row">        
        <p class="pl-4 ml-2">There is a message: @MessageForGrandChildSentFromParent</p>        
        <p class="pl-4 ml-2">Lucky number: @LuckyNumber</p>
    </div>
</div>

    

@code {

    [CascadingParameter(Name = "MessageFromGrandParent")]
    public string MessageForGrandChildSentFromParent { get; set; }
    [CascadingParameter(Name = "LuckyNumberFromGrandParent")]
    public int LuckyNumber { get; set; }

}
